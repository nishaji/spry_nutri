<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item
        android:left="-1dp"
        android:bottom="-1dp"
        android:top="-1dp"
        >
        <shape android:shape="rectangle">
            <stroke android:color="#ffffff" android:width="1dp"/>
        </shape>
    </item>

    <item
        android:left="-1dp"
        android:right="-1dp"
        android:top="-1dp"
        >
        <shape android:shape="rectangle">
            <stroke android:color="#d4dde4" android:width="1dp"/>
        </shape>
    </item>
    <item >
        <selector >
            <item android:state_focused="false" android:state_pressed="true" >
                <shape><solid android:color="@color/MealUnselected"/></shape>
            </item>
        </selector>
    </item>
</layer-list>