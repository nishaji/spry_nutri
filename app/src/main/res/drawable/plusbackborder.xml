<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:right="-2dp"
        android:top="-2dp"
        android:bottom="-2dp"
        >
        <shape
            android:shape="rectangle">
            <solid android:color="#ffffff"
                >
            </solid>
            <stroke android:width="1px" android:color="#d1d1d3" />
            <solid android:color="#f2f4fa"/>

        </shape>

    </item>
    <item > <bitmap android:src="@drawable/plus"/></item>
    <item>
        <selector>
            <item android:state_pressed="true"
                android:drawable="@drawable/statepressd"
                >

            </item>

        </selector>

    </item>


</layer-list>
